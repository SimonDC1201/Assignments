@{
    ViewData["Title"] = "Student";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Phone #</th>
            <th>Course</th>
        </tr>
    </thead>
<tbody>
    @foreach (var item in @Model.Students)
    {
        <tr>
            <th scope="row">@item.StudentId</th>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.EmailAddress</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Course</td>
        </tr>
    }
</tbody>
</table>