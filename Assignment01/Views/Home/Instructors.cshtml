@{
    ViewData["Title"] = "Instructor";
}
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Instructor ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Course</th>
        </tr>
    </thead>
<tbody>
    @foreach (var item in @Model.Instructor)
    {
        <tr>
            <th scope="row">@item.InstructorId</th>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.EmailAddress</td>
            <td>@item.Course</td>
        </tr>
    }
</tbody>
</table>